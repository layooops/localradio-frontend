{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./react-library.json",
  "compilerOptions": {
    "jsx": "preserve",
    "incremental": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "module": "ES2020",
    "target": "es6",
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      },
      {
        "name": "next"
      }
    ]
  }
}
