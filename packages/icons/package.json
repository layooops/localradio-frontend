    "@storybook/testing-library": "0.2.2",
    "@storybook/theming": "7.5.3",
    "@svgr/plugin-prettier": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.9.4",
    "change-case": "5.2.0",
    "fs-extra": "11.1.1",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "storybook": "7.5.3",
    "storybook-addon-tags": "0.0.1",
    "storybook-css-modules": "1.0.8",
    "svgo": "3.0.4",
