{
  "name": "@local-radio/icons",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:source": "tsc",
    "build": "tsup",
    "generate:icons": "yarn build:clean && node --experimental-specifier-resolution=node --loader ts-node/esm ./src/generate.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "type": "module",
  "devDependencies": {
    "@svgr/cli": "8.1.0",
