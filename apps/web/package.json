{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --max-warnings=0 src",
    "lint:css": "stylelint \"**/*.{css,scss,postcss}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss,postcss}\" --fix",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "sitemap": "next-sitemap --config next-sitemap.config.js",
    "codegen": "env-cmd graphql-codegen --config codegen.ts",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@headlessui/react": "^1.7.17",
    "@local-radio/css-tokens": "workspace:*",
    "@local-radio/ui": "workspace:*",
    "@mantine/hooks": "^6.0.19",
    "clsx": "2.0.0",
    "effector": "22.8.7",
    "effector-react": "22.5.4",
    "effector-react-form": "3.0.0-4",
    "effector-view": "2.0.16",
    "graphql": "16.8.1",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-fast-marquee": "1.6.2",
    "react-imask": "7.1.3",
    "react-infinite-scroll-component": "6.1.0",
    "react-player": "2.13.0",
    "react-use-draggable-scroll": "0.4.7",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "sharp": "0.32.6",
    "swr": "2.2.4",
    "tailwind-merge": "2.0.0",
    "tailwind-scrollbar-hide": "1.1.7",
    "typescript": "5.3.2",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@graphql-codegen/cli": "^5.0.0",
    "@local-radio/codegen-config": "workspace:*",
    "@local-radio/eslint-config": "workspace:*",
    "@storybook/addon-essentials": "7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/addon-mdx-gfm": "7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/nextjs": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/testing-library": "0.2.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-input-mask": "^3.0.2",
    "autoprefixer": "10.4.16",
    "env-cmd": "10.1.0",
    "eslint": "8.54.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.1.0",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "0.5.7",
    "storybook": "7.5.3",
    "stylelint": "15.11.0",
    "stylelint-config-css-modules": "4.3.0",
    "stylelint-config-recommended": "13.0.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "tailwindcss": "3.3.5",
    "typescript-plugin-css-modules": "5.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
