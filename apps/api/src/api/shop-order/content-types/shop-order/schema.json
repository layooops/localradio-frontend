{
  "kind": "collectionType",
  "collectionName": "shop_orders",
  "info": {
    "singularName": "shop-order",
    "pluralName": "shop-orders",
    "displayName": "Shop Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "string",
      "required": true
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "postcode": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "items": {
      "type": "component",
      "repeatable": true,
      "component": "order-items.order-items"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "created",
        "payment_success",
        "cancelled",
        "on_the_way",
        "delivered",
        "pickup_arrived",
        "pickuped",
        "delivery_arrived",
        "delivered_finish",
        "refunded"
      ],
      "default": "created"
    }
  }
}
